package com.data;

import java.time.LocalDate;
import java.util.Date;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.boot.ApplicationArguments;
import org.springframework.boot.ApplicationRunner;
import org.springframework.stereotype.Component;

@Component
public class Runners implements ApplicationRunner
{
	@Autowired
	private RepositoryInterface repo;
	
	
	@Override
	public void run(ApplicationArguments args) throws Exception 
	{
		CustomerDataEntity c=new CustomerDataEntity();
		c.setCitizenName("Pramod");
		c.setGender("Male");
		c.setPlanName("Cash");
		c.setPlanStatus("Approved");
		c.setPlanStartDate(LocalDate.now());
		c.setPlanEndDate(LocalDate.now().plusMonths(6));
		c.setBenefitAmt(5000.00);
		
  		
		
		
		
		
	}

}
